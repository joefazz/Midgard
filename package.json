{
    "name": "midgard",
    "version": "1.0.0",
    "description": "server for codexe",
    "main": "index.js",
    "scripts": {
        "start": "ts-node index.ts",
        "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
        "test": "jest"
    },
    "author": "Joe Fazzino",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "dockerode": "^2.5.8",
        "dotenv": "^8.0.0",
        "express": "^4.16.4",
        "express-ws": "^4.0.0",
        "firebase-admin": "^7.3.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "websocket-stream": "^5.4.0"
    },
    "prettier": {
        "singleQuote": false,
        "tabWidth": 4
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
