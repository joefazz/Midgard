        "@types/lodash": "^4.14.126",
        "@types/node": "^12.0.1",
        "@types/passport": "^1.0.0",
        "@types/passport-jwt": "^3.0.1",
        "@types/passport-local": "^1.0.33",
        "@types/ws": "^6.0.1",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "jest": "^24.8.0",
        "nodemon": "^1.18.11",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.3"
    }
